class Solution {

private:
    int getMax(vector<vector<int>>& grid,int i1,int j1) {
        int ans =0;
        for(int i= 0 ; i< 3 ; i++) {

            for(int j = 0 ; j< 3 ; j++) {
                ans = max(ans , grid[i+i1][j+j1]); 
            }
        }

        return ans;
    }    
    
public:
    vector<vector<int>> largestLocal(vector<vector<int>>& grid) {
        int n = grid.size(); 
        vector<vector<int>> ans (n-2, vector<int>(n-2,0));

        for(int i =  0 ; i < n-2 ; i++) {
            for(int j = 0 ;j < n-2 ; j++) {
                ans[i][j] = getMax(grid,i,j); 
            }
        }
        

        return ans; 


    }
};
